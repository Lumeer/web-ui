<!DOCTYPE html>
<html>
  <head>
    <base href="<%= htmlWebpackPlugin.options.publicPath %>">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%= htmlWebpackPlugin.options.title %></title>
    <link rel="stylesheet" type="text/css" href="font-awesome/font-awesome-core.css">
    <link rel="stylesheet" type="text/css" href="font-awesome/font-awesome-brands.css">
    <link rel="stylesheet" type="text/css" href="font-awesome/font-awesome-solid.css">
    <link rel="stylesheet" type="text/css" href="font-awesome/font-awesome-regular.css">
    <link rel="stylesheet" type="text/css" href="font-awesome/font-awesome-light.css">
    <!-- Enable ngx-bootstrap bootstrap 4 theme  -->
    <script>
      window.__theme = 'bs4';
    </script>
    <script type="text/javascript">
      var lumeer = {
        constants: {
          publicPath: '/ui'
        }
      }
    </script>
    <link href="https://cdn.quilljs.com/1.3.4/quill.bubble.css" rel="stylesheet">
    <link href="https://cdn.quilljs.com/1.3.4/quill.snow.css" rel="stylesheet">
  </head>
  <body>
    <lmr-app>
      <div style="height: 95vh; line-height: 95vh; text-align: center; color: #cccccc;">
        <i class="fas fa-spinner fa-spin fa-2x" style="display: inline-block; vertical-align: middle;"></i>
      </div>
    </lmr-app>
    <script src="https://cdn.quilljs.com/1.3.4/quill.js"></script>
    <script src="https://unpkg.com/@reactivex/rxjs@5.5.2/dist/global/Rx.min.js"></script>

    <!-- Bootstrap related Javascripts -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
