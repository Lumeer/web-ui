{
  "name": "@lumeer/web-ui",
  "version": "1.0.0",
  "description": "Lumeer Web UI",
  "main": "index.js",
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=4096 ng build $(node ./env-vars.js)",
    "docs": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d ../web-ui-docs -n 'Lumeer Web UI documentation'",
    "i18n": "npm run i18n-html && npm run i18n-ts && npm run i18n-merge",
    "i18n-html": "ng xi18n --output-path i18n",
    "i18n-ts": "ngx-extractor -i \"src/app/**/*.ts\" -f xlf -o src/i18n/messages.xlf",
    "i18n-merge": "xliffmerge --profile xliffmerge.json en cs",
    "lint": "ng lint lumeer",
    "tslint": "tslint --project ./src/tsconfig.app.json",
    "ng": "ng",
    "reset-env": "echo '{}' > ./src/environments/.env.json",
    "start": "npm run reset-env; ng serve",
    "start:cs": "npm run reset-env; ng serve --configuration=cs",
    "start:en": "npm run reset-env; ng serve --configuration=en",
    "workaround": "sed -i -e 's|/\\*\\*|/*|' node_modules/plotly.js/src/lib/polygon.js",
    "cypress:run": "npx cypress run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lumeer/web-ui.git"
  },
  "license": "GNU GPLv3",
  "bugs": {
    "url": "https://github.com/Lumeer/web-ui/issues"
  },
  "homepage": "https://github.com/Lumeer/web-ui#readme",
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.1",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "angular-resizable-element": "^3.2.0",
    "angulartics2": "^6.2.0",
    "auth0-js": "^9.7.3",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.5",
    "flag-icon-css": "^3.0.0",
    "hammerjs": "^2.0.8",
    "jssha": "^2.3.1",
    "muuri": "^0.6.2",
    "ng-click-outside": "^4.0.0",
    "ng-snotify": "^4.3.1",
    "ngx-contextmenu": "^5.0.2",
    "ngx-infinite-scroll": "^6.0.1",
    "plotly.js": "^1.39.3",
    "raven-js": "^3.26.4",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.1",
    "@angular-devkit/schematics": "^0.7.1",
    "@angular/cli": "^6.1.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@compodoc/compodoc": "^1.1.3",
    "@ngrx/schematics": "^6.0.1",
    "@sentry/cli": "^1.34.0",
    "@types/auth0-js": "^8.11.2",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jssha": "^0.0.29",
    "@types/node": "^8.10.21",
    "@types/plotly.js": "^1.38.5",
    "codelyzer": "^4.4.2",
    "cypress": "^3.1.0",
    "husky": "^1.0.0-rc.15",
    "jasmine": "~3.1.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.2.0",
    "lint-staged": "^7.3.0",
    "ngx-i18nsupport": "^0.16.3",
    "protractor": "~5.4.0",
    "rxjs-tslint-rules": "^4.7.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.ts": [
        "npm run tslint",
        "git add"
      ]
    }
  }
}
