{
  "name": "@lumeer/web-ui",
  "version": "1.0.0",
  "description": "Lumeer Web UI",
  "main": "index.js",
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=4096 ng build $(node ./env-vars.js)",
    "docs": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d ../web-ui-docs -n 'Lumeer Web UI documentation'",
    "i18n": "npm run i18n-html && npm run i18n-ts && npm run i18n-merge",
    "i18n-html": "ng xi18n --output-path i18n",
    "i18n-ts": "ngx-extractor -i \"src/app/**/*.ts\" -f xlf -o src/i18n/messages.xlf",
    "i18n-merge": "xliffmerge --profile xliffmerge.json en cs",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "start:cs": "ng serve --configuration=cs",
    "start:en": "ng serve --configuration=en"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lumeer/web-ui.git"
  },
  "license": "GNU GPLv3",
  "bugs": {
    "url": "https://github.com/Lumeer/web-ui/issues"
  },
  "homepage": "https://github.com/Lumeer/web-ui#readme",
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.2.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "angular-resizable-element": "^3.0.0",
    "angulartics2": "^6.1.0",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.5",
    "hammerjs": "^2.0.8",
    "keycloak-angular": "^3.0.1",
    "muuri": "^0.5.4",
    "ng-click-outside": "^4.0.0",
    "ng-snotify": "^4.3.1",
    "ngx-contextmenu": "^5.0.1",
    "ngx-infinite-scroll": "^6.0.0",
    "ngx-quill": "^3.1.0",
    "parchment": "^1.1.4",
    "plotly.js": "^1.38.1",
    "quill": "^1.3.6",
    "quill-delta-to-html": "^0.8.2",
    "raven-js": "^3.25.2",
    "rxjs": "^6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular/cli": "~6.0.7",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@compodoc/compodoc": "^1.1.3",
    "@ngrx/schematics": "^6.0.1",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jssha": "^0.0.29",
    "@types/node": "^8.10.11",
    "@types/plotly.js": "^1.38.1",
    "@types/quill": "^1.3.6",
    "codelyzer": "^4.3.0",
    "jasmine": "~3.1.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.1.0",
    "ngx-i18nsupport": "^0.16.2",
    "protractor": "~5.3.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2"
  }
}
