      "maxSize": "1379kb"
    },
    {
      "path": "./dist/lumeer/default-src_app_view_perspectives_detail*.js",
      "maxSize": "121kb"
    },
    {
      "path": "./dist/lumeer/default-src_app_view_perspectives_map*.js",
      "maxSize": "1179kb"
    },
    {
      "path": "./dist/lumeer/src_app_view_perspectives_search*.js",
